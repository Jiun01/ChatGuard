{"ast":null,"code":"import SidebarLayout from '@/components/SidebarLayout.vue';\nexport default {\n  name: 'ProfilePage',\n  components: {\n    SidebarLayout\n  },\n  data() {\n    return {\n      replacementsCount: 0\n    };\n  },\n  created() {\n    // Load the replacement count on component creation\n    this.loadReplacementCount();\n  },\n  methods: {\n    loadReplacementCount() {\n      // Check if we're in a Chrome extension environment\n      if (typeof chrome !== 'undefined' && chrome.storage) {\n        chrome.storage.sync.get(['replacementData'], result => {\n          if (result.replacementData) {\n            const data = result.replacementData;\n            const currentDate = new Date();\n            const currentMonth = currentDate.getMonth();\n            const currentYear = currentDate.getFullYear();\n\n            // If the stored month/year matches current month/year, use the stored count\n            // Otherwise, it's a new month, so reset to 0\n            if (data.month === currentMonth && data.year === currentYear) {\n              this.replacementsCount = data.count;\n            } else {\n              this.replacementsCount = 0;\n              // Save the new month data\n              this.saveReplacementCount(0);\n            }\n          } else {\n            // No data stored yet, initialize with 0\n            this.replacementsCount = 0;\n            this.saveReplacementCount(0);\n          }\n        });\n      } else {\n        // Fallback to localStorage for development\n        const storedData = localStorage.getItem('chatguard-replacements');\n        if (storedData) {\n          const data = JSON.parse(storedData);\n          const currentDate = new Date();\n          const currentMonth = currentDate.getMonth();\n          const currentYear = currentDate.getFullYear();\n          if (data.month === currentMonth && data.year === currentYear) {\n            this.replacementsCount = data.count;\n          } else {\n            this.replacementsCount = 0;\n            this.saveReplacementCount(0);\n          }\n        } else {\n          // Initialize with example value for the UI shell (11 as shown in image)\n          this.replacementsCount = 11;\n          this.saveReplacementCount(11);\n        }\n      }\n    },\n    saveReplacementCount(count) {\n      const currentDate = new Date();\n      const data = {\n        count: count,\n        month: currentDate.getMonth(),\n        year: currentDate.getFullYear()\n      };\n      if (typeof chrome !== 'undefined' && chrome.storage) {\n        chrome.storage.sync.set({\n          replacementData: data\n        });\n      } else {\n        localStorage.setItem('chatguard-replacements', JSON.stringify(data));\n      }\n    },\n    logout() {\n      // For the UI shell, just show an alert\n      alert('Logout functionality will be implemented in the future');\n    }\n  }\n};","map":{"version":3,"names":["SidebarLayout","name","components","data","replacementsCount","created","loadReplacementCount","methods","chrome","storage","sync","get","result","replacementData","currentDate","Date","currentMonth","getMonth","currentYear","getFullYear","month","year","count","saveReplacementCount","storedData","localStorage","getItem","JSON","parse","set","setItem","stringify","logout","alert"],"sources":["C:\\Users\\Jiun\\Desktop\\chatguard\\chatguard\\src\\views\\ProfilePage.vue"],"sourcesContent":["<template>\r\n  <SidebarLayout>\r\n    <div class=\"profile-page\">\r\n      <div class=\"profile-info\">\r\n        <div class=\"info-row\">\r\n          <div class=\"info-label\">Replacements made this month:</div>\r\n          <div class=\"info-value\">{{ replacementsCount }}</div>\r\n        </div>\r\n      </div>\r\n      \r\n      <div class=\"profile-links\">\r\n        <a href=\"https://example.com/help\" target=\"_blank\" class=\"link\">Help Center</a>\r\n        \r\n        <button class=\"logout-btn\" @click=\"logout\">\r\n          Log Out\r\n          <span class=\"icon\">â†’</span>\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </SidebarLayout>\r\n</template>\r\n\r\n<script>\r\nimport SidebarLayout from '@/components/SidebarLayout.vue'\r\n\r\nexport default {\r\n  name: 'ProfilePage',\r\n  components: {\r\n    SidebarLayout\r\n  },\r\n  data() {\r\n    return {\r\n      replacementsCount: 0\r\n    }\r\n  },\r\n  created() {\r\n    // Load the replacement count on component creation\r\n    this.loadReplacementCount();\r\n  },\r\n  methods: {\r\n    loadReplacementCount() {\r\n      // Check if we're in a Chrome extension environment\r\n      if (typeof chrome !== 'undefined' && chrome.storage) {\r\n        chrome.storage.sync.get(['replacementData'], (result) => {\r\n          if (result.replacementData) {\r\n            const data = result.replacementData;\r\n            const currentDate = new Date();\r\n            const currentMonth = currentDate.getMonth();\r\n            const currentYear = currentDate.getFullYear();\r\n            \r\n            // If the stored month/year matches current month/year, use the stored count\r\n            // Otherwise, it's a new month, so reset to 0\r\n            if (data.month === currentMonth && data.year === currentYear) {\r\n              this.replacementsCount = data.count;\r\n            } else {\r\n              this.replacementsCount = 0;\r\n              // Save the new month data\r\n              this.saveReplacementCount(0);\r\n            }\r\n          } else {\r\n            // No data stored yet, initialize with 0\r\n            this.replacementsCount = 0;\r\n            this.saveReplacementCount(0);\r\n          }\r\n        });\r\n      } else {\r\n        // Fallback to localStorage for development\r\n        const storedData = localStorage.getItem('chatguard-replacements');\r\n        if (storedData) {\r\n          const data = JSON.parse(storedData);\r\n          const currentDate = new Date();\r\n          const currentMonth = currentDate.getMonth();\r\n          const currentYear = currentDate.getFullYear();\r\n          \r\n          if (data.month === currentMonth && data.year === currentYear) {\r\n            this.replacementsCount = data.count;\r\n          } else {\r\n            this.replacementsCount = 0;\r\n            this.saveReplacementCount(0);\r\n          }\r\n        } else {\r\n          // Initialize with example value for the UI shell (11 as shown in image)\r\n          this.replacementsCount = 11;\r\n          this.saveReplacementCount(11);\r\n        }\r\n      }\r\n    },\r\n    saveReplacementCount(count) {\r\n      const currentDate = new Date();\r\n      const data = {\r\n        count: count,\r\n        month: currentDate.getMonth(),\r\n        year: currentDate.getFullYear()\r\n      };\r\n      \r\n      if (typeof chrome !== 'undefined' && chrome.storage) {\r\n        chrome.storage.sync.set({ replacementData: data });\r\n      } else {\r\n        localStorage.setItem('chatguard-replacements', JSON.stringify(data));\r\n      }\r\n    },\r\n    logout() {\r\n      // For the UI shell, just show an alert\r\n      alert('Logout functionality will be implemented in the future');\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.profile-page {\r\n  display: flex;\r\n  flex-direction: column;\r\n  \r\n  .profile-info {\r\n    margin-bottom: 30px;\r\n    \r\n    .info-row {\r\n      display: flex;\r\n      margin-bottom: 15px;\r\n      \r\n      .info-label {\r\n        width: 200px;\r\n        font-weight: bold;\r\n        color: #001573;\r\n      }\r\n      \r\n      .info-value {\r\n        flex: 1;\r\n      }\r\n    }\r\n  }\r\n  \r\n  .profile-links {\r\n    display: flex;\r\n    flex-direction: column;\r\n    \r\n    .link {\r\n      color: #474DFF;\r\n      margin-bottom: 15px;\r\n      text-decoration: none;\r\n      \r\n      &:hover {\r\n        text-decoration: underline;\r\n      }\r\n    }\r\n    \r\n    .logout-btn {\r\n      display: flex;\r\n      align-items: center;\r\n      background: none;\r\n      border: none;\r\n      color: red;\r\n      cursor: pointer;\r\n      padding: 0;\r\n      font-size: 14px;\r\n      width: fit-content;\r\n      \r\n      .icon {\r\n        margin-left: 5px;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"mappings":"AAuBA,OAAOA,aAAY,MAAO,gCAA+B;AAEzD,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,iBAAiB,EAAE;IACrB;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACC,oBAAoB,CAAC,CAAC;EAC7B,CAAC;EACDC,OAAO,EAAE;IACPD,oBAAoBA,CAAA,EAAG;MACrB;MACA,IAAI,OAAOE,MAAK,KAAM,WAAU,IAAKA,MAAM,CAACC,OAAO,EAAE;QACnDD,MAAM,CAACC,OAAO,CAACC,IAAI,CAACC,GAAG,CAAC,CAAC,iBAAiB,CAAC,EAAGC,MAAM,IAAK;UACvD,IAAIA,MAAM,CAACC,eAAe,EAAE;YAC1B,MAAMV,IAAG,GAAIS,MAAM,CAACC,eAAe;YACnC,MAAMC,WAAU,GAAI,IAAIC,IAAI,CAAC,CAAC;YAC9B,MAAMC,YAAW,GAAIF,WAAW,CAACG,QAAQ,CAAC,CAAC;YAC3C,MAAMC,WAAU,GAAIJ,WAAW,CAACK,WAAW,CAAC,CAAC;;YAE7C;YACA;YACA,IAAIhB,IAAI,CAACiB,KAAI,KAAMJ,YAAW,IAAKb,IAAI,CAACkB,IAAG,KAAMH,WAAW,EAAE;cAC5D,IAAI,CAACd,iBAAgB,GAAID,IAAI,CAACmB,KAAK;YACrC,OAAO;cACL,IAAI,CAAClB,iBAAgB,GAAI,CAAC;cAC1B;cACA,IAAI,CAACmB,oBAAoB,CAAC,CAAC,CAAC;YAC9B;UACF,OAAO;YACL;YACA,IAAI,CAACnB,iBAAgB,GAAI,CAAC;YAC1B,IAAI,CAACmB,oBAAoB,CAAC,CAAC,CAAC;UAC9B;QACF,CAAC,CAAC;MACJ,OAAO;QACL;QACA,MAAMC,UAAS,GAAIC,YAAY,CAACC,OAAO,CAAC,wBAAwB,CAAC;QACjE,IAAIF,UAAU,EAAE;UACd,MAAMrB,IAAG,GAAIwB,IAAI,CAACC,KAAK,CAACJ,UAAU,CAAC;UACnC,MAAMV,WAAU,GAAI,IAAIC,IAAI,CAAC,CAAC;UAC9B,MAAMC,YAAW,GAAIF,WAAW,CAACG,QAAQ,CAAC,CAAC;UAC3C,MAAMC,WAAU,GAAIJ,WAAW,CAACK,WAAW,CAAC,CAAC;UAE7C,IAAIhB,IAAI,CAACiB,KAAI,KAAMJ,YAAW,IAAKb,IAAI,CAACkB,IAAG,KAAMH,WAAW,EAAE;YAC5D,IAAI,CAACd,iBAAgB,GAAID,IAAI,CAACmB,KAAK;UACrC,OAAO;YACL,IAAI,CAAClB,iBAAgB,GAAI,CAAC;YAC1B,IAAI,CAACmB,oBAAoB,CAAC,CAAC,CAAC;UAC9B;QACF,OAAO;UACL;UACA,IAAI,CAACnB,iBAAgB,GAAI,EAAE;UAC3B,IAAI,CAACmB,oBAAoB,CAAC,EAAE,CAAC;QAC/B;MACF;IACF,CAAC;IACDA,oBAAoBA,CAACD,KAAK,EAAE;MAC1B,MAAMR,WAAU,GAAI,IAAIC,IAAI,CAAC,CAAC;MAC9B,MAAMZ,IAAG,GAAI